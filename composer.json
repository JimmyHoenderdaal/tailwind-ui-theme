{
    "name": "rapidez/tailwind-ui-theme",
    "description": "Rapidez Tailwind UI Theme",
    "keywords": [
        "rapidez",
        "Tailwind UI Theme"
    ],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Kevin Meijer",
            "email": "kevin@justbetter.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1|^8.2",
        "laravel/framework": "^10.10",
        "rapidez/core": "^1.0",
        "rapidez/statamic": "^2.6",
        "statamic/cms": "^4.42",
        "statamic/eloquent-driver": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\TailwindUiTheme\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\TailwindUiTheme\\TailwindUiThemeServiceProvider"
            ]
        }
    }
}
